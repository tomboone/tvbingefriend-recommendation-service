[run]
source =
    tvbingefriend_recommendation_service
    scripts

omit =
    */tests/*
    */test_*
    */__pycache__/*
    */site-packages/*
    */scripts/run_pipeline.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
